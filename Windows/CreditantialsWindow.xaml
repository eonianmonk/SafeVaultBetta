        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SafeVaultBetta.Windows"
        mc:Ignorable="d"
        Title="Your accounts" Height="610" Width="556">
